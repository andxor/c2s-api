{
  "name": "c2s-api",
  "version": "0.0.0",
  "description": "Cloud2Sign REST API wrapper",
  "main": "index.js",
  "homepage": "https://github.com/andxor/c2s-api#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/andxor/c2s-api.git"
  },
  "bugs": {
    "url": "https://github.com/andxor/c2s-api/issues"
  },
  "author": "Riccardo Deserti <r.deserti@andxor.it>",
  "license": "ISC",
  "scripts": {
    "test": "node test/test.js local | tap-spec",
    "test-update": "rm -rf test/nock ; node test/test.js | tap-spec"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "tap-spec": "^5.0.0",
    "tape": "^4.9.2",
    "tape-nock": "^1.6.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "strict": [ "error", "global" ],
      "indent": [ "warn", 4 ],
      "linebreak-style": [ "warn", "unix" ],
      "quotes": [ "warn", "single" ],
      "semi": [ "warn", "always" ],
      "comma-dangle": [ "error", "always-multiline" ],
      "no-trailing-spaces": "warn",
      "no-unused-vars": "warn"
    }
  }
}
